<% if user_signed_in? %> 
<h1>Articles</h1>

<ul>
  <% @articles.each do |article| %>
    <li>
      <%= link_to article.title, article %>
    </li>
  <% end %>
</ul>
<%= link_to "New Article", new_article_path %>
<% end %> 
<% if !user_signed_in? %>
<h3>You are not logged in. <%= link_to "Login here.", new_user_session_path %></h3>
<% end %>